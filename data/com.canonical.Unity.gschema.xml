<schemalist>
  <!--
  Fake schema for com.canonical.Unity to get access to the
  integrated-menus setting in sandboxed packages (Flatpak, Snap).
  -->

  <schema path="/com/canonical/unity/" id="com.canonical.Unity" gettext-domain="unity">
    <key type="b" name="integrated-menus">
      <default>false</default>
    </key>
  </schema>
</schemalist>
